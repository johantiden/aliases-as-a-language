#!/usr/bin/env bash

source "$(dirname $0)/../impl.sh"
__debugecho ": remove one exact (auto)file(s)~"

target=$(__aaal_list_all_files_from_home | __aaal_one_exact);
if [[ -z "$target" ]]; then
  __debugecho abort
  exit 0;
fi

if ask "'rm $target', continue?" N; then
  #rm $target
  echo DRYRUN: rm $target
fi;