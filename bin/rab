#!/usr/bin/env bash

source "$(dirname $0)/../impl.sh"
__debugecho ": remove all branch(es)"

count=$(__impl_get_all_branches | wc -l)

if ask "Count is $count, continue?" Y; then
  target=$(__impl_get_all_branches);
if ask "'rm $target', continue?" N; then
  #rm $target
  echo DRYRUN: rm $target
fi;
fi;