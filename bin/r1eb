#!/usr/bin/env bash

source "$(dirname $0)/../impl.sh"
__debugecho ": remove one exact (auto)branch(es)"

target=$(__impl_get_all_branches | __impl_one_exact);
if [[ -z "$target" ]]; then
  __debugecho abort
  exit 0;
fi

if ask "'rm $target', continue?" N; then
  #rm $target
  echo DRYRUN: rm $target
fi;