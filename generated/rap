#!/usr/bin/env bash

source "$(dirname $0)/../impl.sh"
__debugecho ": remove all pasting(s)"

count=$(clipL | wc -l)

if ask "Count is $count, continue?" Y; then
  target=$(clipL);
if [[ -z "$target" ]]; then
  __debugecho abort
  exit 0;
fi

if ask "'rm $target', continue?" N; then
  #rm $target
  echo DRYRUN: rm $target
fi;
fi;